{
  "name": "news-aggregator",
  "version": "1.0.0",
  "description": "news-aggregator API",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node lib/index.js",
    "start:dev": "ts-node-dev --respawn src/*.ts",
    "start:lambda": "serverless offline start --skipCacheInvalidation",
